{
  "name": "next-serve-side",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bootstrap": "yarn install; yarn --cwd functions install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start-functions": "yarn --cwd functions start",
    "build-functions": "yarn --cwd functions run build",
    "serve-functions": "yarn --cwd functions run serve",
    "shell": "yarn --cwd functions run shell",
    "lint": "next lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "next-manage": "rimraf ./functions/.next && cp -a .next ./functions/.next",
    "deploy-dev": "firebase deploy --project dev"
  },
  "dependencies": {
    "antd": "5.1.2",
    "axios": "0.27.2",
    "eslint-config-next": "13.1.1",
    "firebase": "9.9.3",
    "i18next": "22.4.8",
    "i18next-browser-languagedetector": "7.0.1",
    "next": "^13.1.1",
    "next-images": "^1.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "12.1.1",
    "react-query": "^3.39.2",
    "rimraf": "3.0.2",
    "styled-components": "^5.3.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,sass,ts,tsx}": [
      "eslint -c .eslintrc.js --ignore-path .eslintignore --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.20.7",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "babel-loader": "8.3.0",
    "eslint": "8.3.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.7.1",
    "stylelint": "14.16.1",
    
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "4.7.4"
  }
}
