{
  "name": "functions",
  "scripts": {
    "build": "yarn lint && concurrently \"yarn:build-tsc\"  \"yarn:build-pug\"",
    "build-tsc": "tsc",
    "build-pug": "cpx src/**/*.pug lib",
    "lint": "tslint --project tsconfig.json",
    "start": "NODE_ENV=local concurrently \"yarn:start-tsc\" \"yarn:start-pug\"",
    "start-w": "set NODE_ENV=local && concurrently \"yarn:start-tsc\" \"yarn:start-pug\"",
    "start-tsc": "tsc --watch",
    "start-pug": "cpx src/**/*.pug lib --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "NODE_ENV=local firebase functions:shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "antd": "5.1.2",
    "axios": "0.27.2",
    "eslint-config-next": "13.1.1",
    "firebase": "9.9.3",
    "i18next": "22.4.8",
    "i18next-browser-languagedetector": "7.0.1",
    "react-i18next": "12.1.1",
    "react-query": "3.39.2",
    "cors": "2.8.5",
    "express": "4.18.2",
    "express-validator": "6.14.2",
    "firebase-admin": "11.4.1",
    "firebase-functions": "4.1.1",
    "next": "^13.1.1",
    "next-images": "^1.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.26",
    "@types/express": "4.17.15",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "concurrently": "^7.6.0",
    "cpx": "1.5.0",
    "eslint": "8.9.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-import": "2.25.4",
    "firebase-functions-test": "0.2.0",
    "tslint": "6.1.3",
    "typescript": "4.7.4"
  },
  "private": true
}
